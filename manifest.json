{
    "manifest_version": 3,
    "name": "Roblox Play Button | Back to green",
    "version": "1.0",
    "description": "Changes the color of the play button on Roblox to green.",
    "content_scripts": [
      {
        "matches": ["*://*.roblox.com/*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "permissions": ["scripting"]
}
